{
  "manifest_version": 2,
  "name": "String to JSON Converter",
  "version": "1.0",
  "description": "Converts strings to JSON format",
  "permissions": ["activeTab", "declarativeContent"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
